<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:App.Views"
             x:Class="App.Views.MainPage">
    <Grid Padding="0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.2*"/>
            <ColumnDefinition Width="0.7*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Frame Margin="20" CornerRadius="15" BackgroundColor="{StaticResource Gray30}" VerticalOptions="Fill" HorizontalOptions="Fill"></Frame>
        
        <views:Agenda Grid.Column="1" />
    </Grid>
    
    
</ContentPage>
